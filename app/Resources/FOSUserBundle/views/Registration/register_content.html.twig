                    {{ form_widget(form.username, { 'attr': {'class': 'form-input', 'placeholder': 'Your Email','type':'text','name':'name','id':'email' } }) }}
                    <label>Password :</label>
                    <span> </span>

                    {{ form_widget(form.plainPassword.first, { 'attr': {'class': 'form-input col-md-12 col-sm-12  ', 'placeholder':'Password','type':'text','name':'password','id':'email' } }) }}
                    <span> </span>
                    <label> Repeat password : </label>
                    <span> </span>

