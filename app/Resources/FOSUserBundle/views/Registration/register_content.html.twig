                    <div class="form-group" id="fos_user_registration_form">



                    <label>Email : </label>




                    {{ form_widget(form.email, { 'attr': {'class': 'form-input', 'placeholder': 'Your Email','type':'email','name':'email','id':'email' } }) }}
